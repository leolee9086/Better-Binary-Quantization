{
  "name": "@leolee9086/better-binary-quantization",
  "version": "1.0.0",
  "description": "基于Lucene的二值量化实现，提供优化的向量量化和搜索功能",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage",
    "bench": "vitest bench  --run",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run"
  },
  "keywords": [
    "binary-quantization",
    "vector-search",
    "lucene",
    "machine-learning",
    "similarity-search",
    "quantization",
    "nearest-neighbor",
    "vector-embeddings"
  ],
  "author": "leolee9086",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leolee9086/better-binary-quantization.git"
  },
  "bugs": {
    "url": "https://github.com/leolee9086/better-binary-quantization/issues"
  },
  "homepage": "https://github.com/leolee9086/better-binary-quantization#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "publishConfig": {
    "access": "public"
  }
} 